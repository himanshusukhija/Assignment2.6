import java.util.Scanner;
public class acad {
public static void main(String a[]) {
 StringBuffer sb1 = new StringBuffer(); //initiate buffer 
StringBuffer sb2 = new StringBuffer();//initiate buffer 
Scanner s =new Scanner(System.in); 
 int y = s.nextInt(); //declare variable and scan value from user
 int z =s.nextInt(); //declare variable and scan value from user 
for (int i = a; i <=b; i++) { // run loop for chaecking even and odd
 if(i%2==0) //check if number is even
 { sb1.append(i); sb1.append(" "); //store in buffer sb1 }
 else { sb2.append(i); sb2.append(" "); //if odd store in buffer sb2 } }
 System.out.println("The even number are"); //print values in buffer sb 
 System.out.println(sb.toString());
 System.out.println("The odd number are"); //print values in buffer sb2
 System.out.println(sb1.toString()); }
} 

2.
import java.util.Scanner;
public class acad {
public static void main(String args[]) {
 Scanner sc= new Scanner(System.in); // taking input from user 
System.out.println("Input"); //displays output
int no = sc.nextInt(); //scan taking values from user 
System.out.println("Output"); 
for (int i = 1; i <=10; i++)
 { System.out.println(no+"x"+i+"="+i*no); //calculates and prints 10 multiples
    }
}
}


